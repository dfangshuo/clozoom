{
    "manifest_version": 3,
    "name": "Clozoom",
    "short_name": "Clozoom",
    "version": "1.1.0",
    "description": "Close your Zoom meeting tabs automatically!",
    "icons": {},
    "icons": {
        "16": "icons/16.png",
        "48": "icons/48.png",
        "64": "icons/64.png",
        "128": "icons/128.png"
    },
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_title": "Clozoom",
        "default_popup": "popup.html"
    },
    "homepage_url": "https://thesephist.github.io/clozoom/",
    "offline_enabled": true,
    "permissions": [
        "https://*.zoom.us/j/*",
        "tabs",
        "storage"
    ]
}
