{
    "manifest_version": 2,
    "name": "Clozoom",
    "version": "1.0.0",
    "description": "Close your Zoom meeting tabs automatically!",
    "icons": {},
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "browser_action": {
        "default_popup": "popup.html"
    },
    "homepage_url": "https://github.com/thesephist/clozoom",
    "offline_enabled": true,
    "permissions": [
        "https://*.zoom.us/j/*",
        "tabs",
        "storage"
    ],
    "short_name": "Clozoom"
}
